<form action="/hrc/employee/fillSalaryForm" method="POST">
    <label for="employeeId" id="labelemployeeId" hidden>Task Type:</label>
    <input type="text" class="employeeId" id="employeeId" name="employeeId" value="{{employeeId}}" readonly="readonly"
        hidden><br>
    <label for="taskId" id="labelTaskId" hidden>Task Type:</label>
    <input type="text" class="taskId" id="taskId" name="taskId" value="{{taskId}}" readonly="readonly"
        hidden><br>
    
    <label for="ssn">SSN:</label>
    <input type="number" id="ssn" name="ssn" required>

    <label for="accountNo">Account Number:</label>
    <input type="number" id="accountNo" name="accountNo" required>

    <label for="routingNo">Routing Number:</label>
    <input type="number" id="routingNo" name="routingNo" required>

    <label for="billingAddress">Billing Address:</label>
    <input type="text" id="billingAddress" name="billingAddress" required>

    <label for="paymentType">Payment Type:</label>
    <select id="paymentType" name="paymentType" required>
        <option value="">--Please choose an option--</option>
        <option value="direct deposit">Direct deposit</option>
        <option value="check">Check</option>
    </select>

    <input type="submit" value="Submit">
</form>
<button onclick="window.location.href='/hrc/employee/getAllToDoByEmpId'" id="myToDoTasks">Back To tasks</button>
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script src="/public/js/salary.js">
</script>